.gray_line {
  width: 100%;
  height: 20rpx;
  background-color: #f5f7fa;
}

.flex {
  display: flex;
}

.border_radius {
  border-radius: 5px;
}

.touch_item {
  width: 100%;
  height: 116rpx;
  font-size: 14px;
  display: flex;
  align-items: center;
  background-color: #fff;
  overflow: hidden;
}

.content {
  width: 100%;
  line-height: 22px;
  margin-right: 0;
  padding-left: 40rpx;
  /* -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px; */
  box-sizing: content-box;
}

.content_company {
  font-size: 14px;
  color: #4a4a4a;
}

.content_time {
  font-size: 12px;
  color: #979797;
}

.del {
  background-color: orangered;
  width: 74px;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  color: #fff;
  -webkit-transform: translateX(74px);
  transform: translateX(74px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

/* remove scroll-view scroll-bar */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}


.create_apply {
  width: 128rpx;
  height: 128rpx;
  text-align: center;
  background-color: #FFFFFF;
  font-size: 16px;
  color: #979797;
  border-radius: 100%;
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 999999999999999999999999999 !important;
  /* box-shadow: #3242a0ff 0 0 6px; */
}

.create_apply_inner {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin: 14rpx auto;
  /* border: 1px solid #2b2b2b; */
  border-radius: 100%; 
  background: linear-gradient(to bottom,#73c0ff, #0675f9);
  font-size: 26px;
  color: #fff;
}

.create_apply_inner_circul {
  width: 48rpx;
